<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    tools:context=".ActionbarFragment">

    <Button
        android:id="@+id/settings"
        android:layout_width="94dp"
        android:layout_height="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="14dp"

        android:background="@drawable/gear_duotone">

    </Button>

    <Button
        android:id="@+id/weather"
        android:layout_width="94dp"
        android:layout_height="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="368dp"
        android:background="@drawable/cloud_sun_duotone">

    </Button>

    <Button
        android:id="@+id/hiker"
        android:layout_width="94dp"
        android:layout_height="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="488dp"
        android:background="@drawable/person_simple_run_duotone">

    </Button>

    <Button
        android:id="@+id/health"
        android:layout_width="94dp"
        android:layout_height="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="125dp"
        android:background="@drawable/chart_bar_duotone">

    </Button>

    <Button
        android:id="@+id/home"
        android:layout_width="94dp"
        android:layout_height="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="246dp"
        android:background="@drawable/house_duotone"></Button>
</RelativeLayout>