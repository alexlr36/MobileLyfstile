<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mapsFrag">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        tools:ignore="UselessParent">

        <TextView
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="69dp"
            android:fontFamily="@font/shippori"
            android:gravity="center"
            android:background="#0000"
            android:text="What do you want to do?"
            android:textSize="30sp"
            tools:ignore="HardcodedText" />

        <RelativeLayout
            android:id="@+id/action_bar_fragment"
            android:layout_width="match_parent"
            android:layout_height="83dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="1dp"
            android:layout_marginTop="648dp"
            android:layout_marginEnd="-1dp">

        </RelativeLayout>

        <Button
            android:id="@+id/Maps_Gym"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:layout_below="@+id/Maps_Parks"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="105dp"
            android:text="Gyms"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/Maps_Hiking"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="180dp"
            android:layout_marginEnd="104dp"
            android:text="Hiking"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/Maps_Parks"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:layout_below="@+id/Maps_Hiking"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="105dp"
            android:text="Parks"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/Maps_Restaurants"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:layout_below="@+id/Maps_Gym"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="106dp"
            android:text="Restaurants"
            tools:ignore="HardcodedText" />
    </RelativeLayout>
</FrameLayout>